{
    "name": "lacasera/http-request-response-logger",
    "description": "A Laravel package to log HTTP requests",
    "keywords": [
        "64robots",
        "laravel-http-logger",
        "response-logger",
        "cloudwatch",
        "aws"
    ],
    "homepage": "https://github.com/64robots/laravel-cloudwatch-http-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "Agyenim Boateng",
            "email": "agyenim@64robots.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "R64Robots\\HttpLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "R64Robots\\HttpLogger\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "R64Robots\\HttpLogger\\HttpLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
